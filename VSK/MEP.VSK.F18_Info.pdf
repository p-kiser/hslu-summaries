<script language="JavaScript">
$(document).ready
(
    function()
    {
        $("input[name='cmd[doStandardAuthentication]']").attr("onclick","correctIliasLogin()");
		$("#username").val() ? $("#password").focus() : $("#username").focus();
    }
)

function correctIliasLogin()
{
    if ($("#username").val().toLowerCase().substr(0, 7) == 'campus\\' || $("#username").val().toLowerCase().substr(0, 7) == 'campus/') {
        $("#username").val($("#username").val().substr(7, 100))};
}
</script>


				
